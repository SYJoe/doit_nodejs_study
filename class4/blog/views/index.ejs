<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>Posts</title>
  </head>
  <body>
    <h1>Posts</h1>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Title</th>
          <th>Author</th>
        </tr>
      </thead>
      <tbody id="post-list">
        <% data.forEach(post => { %>
          <tr>
            <td><%= post.id %></td>
            <td>
              <a href="/posts/<%= post.id %>"><%= post.title %></a>
              <td><%= post.author %></td>
            </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
